APP_NAME="TronLive"
APP_ENV=production
APP_KEY=base64:GENERATE_NEW_KEY_HERE
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=http://yourdomain.com

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=null
LOG_STACK=null
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=emergency

# Hostinger Database Configuration
DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=your_database_name_here
DB_USERNAME=your_username_here
DB_PASSWORD=your_password_here

# Hostinger-optimized cache settings (file cache for shared hosting)
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync

# Cache optimized for Hostinger shared hosting
CACHE_STORE=file
CACHE_PREFIX=tronlive

# Mail configuration (configure based on your needs)
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# TronGrid API Configuration
TRONGRID_API_KEY=your_trongrid_api_key_here
TRONGRID_BASE_URL=https://api.trongrid.io

# Admin Wallet Configuration (for withdrawals) - MOVE TO ENCRYPTED DATABASE
ADMIN_WALLET_PRIVATE_KEY=BFD03301FB14BE68C6C646E10D67E45878935F8A4ABD2F618684A7745C42257E
ADMIN_WALLET_ADDRESS=TL8548WGPxkUbneCLJwg1UgQvNeNMe7E96

# Wallet Encryption (generate random salt)
WALLET_ENCRYPTION_SALT=your_random_encryption_salt_here_64_chars_minimum

# Google 2FA Configuration
GOOGLE_2FA_ENABLED=true

# Application specific settings
MINIMUM_WITHDRAWAL=10
REFERRAL_COMMISSION_RATE=0.05

# Hostinger-specific optimizations
HOSTINGER_MEMORY_LIMIT=256M
HOSTINGER_MAX_EXECUTION_TIME=300
HOSTINGER_CHUNK_SIZE=25

# Performance settings for shared hosting
DB_SLOW_QUERY_TIME=2
VIEW_COMPILED_PATH="${APP_PATH}/storage/framework/views"

# Security settings
SECURE_HEADERS=true
FORCE_HTTPS=false 